{
  "name": "doc-version-redirector",
  "version": "0.12.7",
  "description": "Automatically redirect to a specific documentation version",
  "engines": {
    "node": ">=14.0.0"
  },
  "eslintIgnore": [
    "dist/"
  ],
  "scripts": {
    "build": "webpack --config conf/webpack.prod.js",
    "dev": "webpack --config conf/webpack.dev.js",
