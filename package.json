  "devDependencies": {
    "@types/jest": "^25.2.2",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.0.0",
    "gts": "^2.0.2",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
