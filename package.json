  "description": "Automatically redirect to a specific documentation version",
  "engines": {
    "node": ">=13.0.0"
  },
  "eslintIgnore": [
    "dist/"
  ],
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack --config webpack.dev.js",
