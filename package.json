    "typescript-tuple": "^2.2.1",
    "webextension-polyfill-ts": "^0.19.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5",
    "zip-webpack-plugin": "^3.0.0"
