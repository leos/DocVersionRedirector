    "@types/jest": "^26",
    "@types/node": "^14",
    "@typescript-eslint/eslint-plugin": "^3.7",
    "@typescript-eslint/parser": "^3.7",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "eslint": "^7.0.0",
    "gts": "^2.0.2",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.1",
    "json-schema": "^0.2.5",
    "sass-loader": "^9.0.2",
